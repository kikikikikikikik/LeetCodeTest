class Solution {
    public boolean isIsomorphic(String s, String t) {
        char[] c1=s.toCharArray();
        char[] c2=t.toCharArray();
        for(int i=0;i<s.length();i++){
            if(s.indexOf(c1[i])!=t.indexOf(c2[i])){
                return false;
            }
        }
        return true;
    }
}

import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        while(sc.hasNext()){
            String str = sc.nextLine();
            if(str.startsWith("0x")){
                str = str.substring(2);
            }
            Integer i = Integer.valueOf(str,16);
            System.out.println(i);
        }
    }
}
